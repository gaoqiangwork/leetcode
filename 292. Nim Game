//解法1
class Solution{
public:
	bool canWinNim(int n){
		if(n < 4)
		{
			return true;
		}
		int m = (n - 4) % 4;
		if(!m)
		{
			return false;
		}
		else 
		{
			return true;
		}
	}
}



//解法2
class Solution {
public:
    bool canWinNim(int n) {
		return (n % 4 != 0);
    }
};