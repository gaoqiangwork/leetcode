class Solution {
public:
    vector<string> fizzBuzz(int n) {
        vector<string> fizzBuzz;
        for(int i = 0; i <= n; ++i)
		{
			if((i % 3 == 0) && (i % 5 == 0))
				fizzBuzz.push_back("FizzBuzz");
			if(i % 3 == 0)
				fizzBuzz.push_back("Fizz");
			if(i % 5 == 0)
				fizzBuzz.push_back("Buzz");
			fizzBuzz.push_back(to_string(i));
		}
		return fizzBuzz;
    }
};